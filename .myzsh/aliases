# Enhanced WHOIS lookups
alias whois="whois -h whois-servers.net"


alias hello="echo Hello World"

alias mountdat="truecrypt -t --mount /media/dat/Crypted\ Volumes/BlackKingdom ~/.truecrypt"
alias dismountdat="truecrypt -t -d ~/.truecrypt"

alias apachestop="sudo /etc/init.d/apache2 stop"
alias apachestart="sudo /etc/init.d/apache2 start"
alias apacherestart="sudo /etc/init.d/apache2 restart"
